<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>

<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="200.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.file_system_simulator.HelloController">
    <padding>
        <Insets bottom="200.0" left="300.0" right="300.0" top="200.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <Button text="Hello!" onAction="#onHelloButtonClick"/>
    <TextField fx:id="textField" text="World" onAction="#onTextClick"/>
</VBox>
